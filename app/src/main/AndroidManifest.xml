<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.akapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".controllers.LoadingScreenActivity_"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".controllers.NewsFeedActivity_" />
        <activity android:name=".controllers.LoginAndRegisterActivity_" />
        <activity android:name=".controllers.ChatActivity_" />
        <activity android:name=".controllers.CalendarActivity_" />
        <activity android:name=".controllers.SettingsActivity_">
        </activity>
    </application>

</manifest>